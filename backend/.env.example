# Local development — copy to .env and fill in real values
# Production values go in Cloud Run / Secret Manager — never commit real secrets

JDBC_URL=jdbc:postgresql://localhost:5432/ads
PSQL_USER=ads_user
PSQL_PASSWORD=ads_pass

# Generate: openssl rand -base64 32
JWT_SECRET=your-jwt-secret-min-256-bits

GOOGLE_CLIENT_ID=your-google-client-id.apps.googleusercontent.com

# Comma-separated origins (no trailing slashes)
CORS_ALLOWED_ORIGINS=http://localhost:3000,http://localhost:8081
